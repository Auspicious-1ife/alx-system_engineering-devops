# Default server configuration
#
server {
        listen 80 default_server;
        listen [::]:80 default_server;
        server_name 3.83.253.53
        add_header X-Served-By 217081-web-01;
        location = /airbnb-onepage/ {
                proxy_pass http://127.0.0.1/airbnb-onepage/;
        }

        error_page 404 /404.html;
        location /404 {
          root /var/www/html;
          internal;
        }
        }
